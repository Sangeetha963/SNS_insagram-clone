  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rimraf dist && npx swc src -d dist",
    "cmd": "ts-node -r tsconfig-paths/register --project tsconfig.json --transpile-only src/cmd.ts",
    "cmd:prod": "cross-env RUNNING_JS=true node dist/cmd.js",
    "doc": "compodoc",
    "doc:open": "opener ./.pages/doc/index.html",
    "fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\"",
    "postinstall": "patch-package",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js,json}\"",
    "prepare": "is-ci || husky install",
    "start": "nodemon",
    "start:prod": "cross-env RUNNING_JS=true node dist/main",
    "test": "jest",
    "test:ci": "jest --ci --coverage --forceExit",
    "test:cov": "jest --coverage",
    "test:cov:result": "opener ./.pages/cov/index.html",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typeorm": "ts-node --transpile-only -r tsconfig-paths/register $(npm bin)/typeorm -d src/data-source.ts",
    "typeorm:create": "npm run typeorm migration:generate",
    "typeorm:down": "npm run typeorm migration:revert",
    "typeorm:prod": "node $(npm bin)/typeorm -d dist/data-source.js",
    "typeorm:prod:create": "npm run typeorm:prod migration:generate",
    "typeorm:prod:down": "npm run typeorm:prod migration:revert",
    "typeorm:prod:up": "npm run typeorm:prod migration:run",
    "typeorm:test": "cross-env NODE_ENV=test npm run typeorm",
    "typeorm:test:up": "cross-env NODE_ENV=test npm run typeorm:up",
    "typeorm:up": "npm run typeorm migration:run"
  },
